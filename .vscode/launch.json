  "version": "0.2.0",
  "configurations": [
    {
      "name": "webcv",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "webcv (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "webcv (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Flutter Web Renderer: HTML",
      "request": "launch",
      "type": "dart",
      "args": ["--web-renderer", "html"]
